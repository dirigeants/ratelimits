{
  "name": "@klasa/ratelimits",
  "version": "0.0.1",
  "description": "Handles cooldowns and slowmodes for klasa.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "test": "ava",
    "test:lint": "eslint --ext ts src test",
    "test:coverage": "npx nyc check-coverage --lines 90 --functions 90 --branches 90",
    "coverage": "npx nyc --require ts-node/register --require source-map-support/register npm test",
    "coverage:report": "npx nyc report --reporter=html",
    "lint": "eslint --fix --ext ts src test"
  },
  "keywords": [],
  "author": "dirigeants",
  "license": "MIT",
  "engines": {
    "node": ">=10.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@klasa/utils": "^0.0.4",
    "@types/node": "^13.9.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "ava": "^3.5.1",
    "discord.js": "discordjs/discord.js",
    "eslint": "^6.8.0",
    "eslint-config-klasa": "dirigeants/klasa-lint",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dirigeants/ratelimits"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*.ts",
      "!test/lib"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.5"
  }
}
